<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>NameListGet</servlet-name>
        <servlet-class>edu.simpson.brooke.NameListGet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListGet</servlet-name>
        <url-pattern>/api/name_list_get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameListEdit</servlet-name>
        <servlet-class>edu.simpson.brooke.NameListEdit</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>NameListEdit</servlet-name>
        <url-pattern>/api/name_list_edit</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>NameListDelete</servlet-name>
        <url-pattern>/api/name_list_delete</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NameListDelete</servlet-name>
        <servlet-class>edu.simpson.brooke.NameListDelete</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GetLoginServlet</servlet-name>
        <url-pattern>/api/get_login_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetLoginServlet</servlet-name>
        <servlet-class>edu.simpson.brooke.GetLoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/api/login_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>edu.simpson.brooke.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>InvalidateSessionServlet</servlet-name>
        <url-pattern>/api/invalidate_session_servlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InvalidateSessionServlet</servlet-name>
        <servlet-class>edu.simpson.brooke.InvalidateSessionServlet</servlet-class>
    </servlet>

</web-app>